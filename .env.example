# Google AI API Key for Gemini models
GOOGLE_API_KEY=your_google_api_key_here

# Amap (高德地图) API Key for location services and POI data
# Get your key from: https://console.amap.com/
# Note: Already configured in travel_agent/.env as AMAP_MAPS_API_KEY
AMAP_MAPS_API_KEY=your_amap_api_key_here

# Optional: Weather API keys for enhanced weather data
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Optional: Travel booking API keys
BOOKING_API_KEY=your_booking_api_key_here
AGODA_API_KEY=your_agoda_api_key_here

# MCP Server Configuration
MCP_LOG_LEVEL=info
MCP_TIMEOUT=30000
MCP_RETRIES=3

# Travel Agent Settings
DEFAULT_TIMEZONE=Asia/Shanghai
DEFAULT_CURRENCY=CNY
CACHE_ENABLED=true
CACHE_TTL=3600
